									"path": "['properties']['typeProperties']['fileName']"
								},
								"sink": {
									"type": "String",
									"ordinal": 8
								}
							},
							{
								"source": {
									"path": "['properties']['typeProperties']['folderPath']"
								},
								"sink": {
									"type": "String",
									"ordinal": 9
								}
							},
							{
