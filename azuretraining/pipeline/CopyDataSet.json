				"inputs": [
					{
						"referenceName": "datasetJSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "datasetPIPE",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Copy data2",
