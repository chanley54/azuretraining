								}
							},
							{
								"source": {
									"path": "['properties']['linkedServiceName']['type']"
								},
								"sink": {
									"type": "String",
									"ordinal": 4
								}
							},
							{
								"source": {
									"path": "['properties']['linkedServiceName']['referenceName']"
								},
								"sink": {
									"type": "String",
									"ordinal": 5
								}
							},
							{
								"source": {
									"path": "['properties']['annotations']"
								},
								"sink": {
									"type": "String",
