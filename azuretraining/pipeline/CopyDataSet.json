								"sink": {
									"type": "String",
									"ordinal": 1
								}
							},
							{
								"source": {
									"path": "['name']"
								},
								"sink": {
									"type": "String",
									"ordinal": 2
								}
							},
							{
								"source": {
									"path": "['type']"
								},
								"sink": {
