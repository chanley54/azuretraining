{
	"name": "plParquet",
	"properties": {
		"activities": [
			{
				"name": "copyparquet",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowCopyCommand": true
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "TEST_DATE",
									"type": "DateTime"
								},
								"sink": {
									"name": "TEST_DATE",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"name": "TEST_INT",
									"type": "String"
								},
								"sink": {
									"name": "TEST_INT",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "TEST_NVAR",
									"type": "String"
								},
								"sink": {
									"name": "TEST_NVAR",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Parquet1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "tgt_chanleypool",
						"type": "DatasetReference"
					}
				]
			}
		],
		"concurrency": 2,
		"annotations": []
	}
}